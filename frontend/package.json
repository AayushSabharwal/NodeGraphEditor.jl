{
  "name": "nodegraph",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.24.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^5.5.5",
    "preact": "^10.6.4",
    "sass": "^1.45.0",
    "typescript": "^4.5.3"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "scripts": {
    "clean": "rimraf ../public/[!.gitkeep]*",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --dist-dir ../public/",
    "serve": "npx serve ../public/"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "@types/file-saver": "^2.0.4",
    "parcel": "^2.0.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "serve-static": "^1.14.2"
  }
}